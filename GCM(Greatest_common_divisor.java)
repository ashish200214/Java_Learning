public class GCM {
    static void gcm(int a,int b)
    {
        int count=1,i;
        for(i=1;i<=a;i++)
        {
            if(a%i==0 && b%i==0)
            {
                count=i;
            }
        }
        
        System.out.print("GCD is "+count);
    }
    public static void main(String[] args) {
        gcm(8,20);
    }
}
