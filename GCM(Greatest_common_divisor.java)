public class GCD {
    static void gcd_fun(int a,int b)
    {
        int gcd=1,i;
        for(i=1;i<=a;i++)
        {
            if(a%i==0 && b%i==0)
            {
                gcd=i;
            }
        }
        
        System.out.print("GCD is "+gcd);
    }
    public static void main(String[] args) {
        gcd_fun(8,20);
    }
}
